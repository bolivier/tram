{:aliases
 {:dev       {:extra-deps  {babashka/process       {:mvn/version "0.5.21"}
                            io.github.nubank/morse {:git/sha "d99b09c"
                                                    :git/tag "v2023.04.30.01"}}
              :extra-paths ["dev"]}
  :test      {:extra-deps  {lambdaisland/kaocha         {:mvn/version
                                                         "1.91.1392"}
                            nubank/matcher-combinators  {:mvn/version "3.9.2"}
                            pjstadig/humane-test-output {:mvn/version "0.11.0"}}
              :extra-paths ["test"]
              :jvm-opts    ["--add-opens=java.base/java.nio=ALL-UNNAMED"
                            "-Dio.netty.tryReflectionSetAccessible=true"]
              :main-opts   ["-m" "kaocha.runner"]}
  :watch     {:exec-args {:watch? true}
              :exec-fn   kaocha.runner/exec-fn}
  ;; This alias is useful if you need to develop a new hook for a macro
  :clj-kondo {:extra-deps {clj-kondo/clj-kondo {:mvn/version "LATEST"}}}
  :build     {:extra-deps {io.github.clojure/tools.build {:mvn/version "0.9.6"}}
              :ns-default build}}

