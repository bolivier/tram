#!/usr/bin/env bb

(require '[tram-cli.nrepl-client :as client])
(require '[zprint.core :refer [zprint-file-str]])
(require '[clojure.string :as str])

(defn -main [& args]
  (let [cmd (first args)]
    (case cmd
      "new" (println "not yet implemented")
      (client/send {:op  :tram/op
                    :cmd cmd}))))

(apply -main *command-line-args*)
