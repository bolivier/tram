#kaocha/v1
 {:default-test-paths ["test/tram"]
  :kaocha.hooks/pre-load [kaocha-utils.kaocha-hooks/load-matcher-combinators
                          kaocha-utils.kaocha-hooks/set-up-test-database]
  :plugins [:kaocha.plugin/hooks]
  :tests [{:id          :unit
           :ns-patterns ["^tram." "^rapid-test."]
           :test-paths  ["test/main" "test/bb_compatible"]}
          {:id          :cli
           :ns-patterns ["^tram_cli"]
           :test-paths  ["test/bb_only/tram_cli" "test/bb_compatible"]}]}
